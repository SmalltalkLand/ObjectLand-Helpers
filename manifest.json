{
  "name": "ObjectLand",
  "version": "1.0",
  "description": "ObjectLand helpers",
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["Squeak/squeak.js","background.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": ["https://objectlandschool.gkgoat.repl.co/"]
  },
  "content_scripts":[{"matches": ["https://*/*"],"js": ["jquery.js","jquery-ui.min.js","vue.js","content.js"]}],
    "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
